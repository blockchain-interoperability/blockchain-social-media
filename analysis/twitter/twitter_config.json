{
    "cache_index": {
        "hostname": "http://idea-vm-elasticsearch:9200",
        "index_name": "blockchain-interoperability-attacks",
        "snapshot_path": "/data/blockchain-interoperability/blockchain-social-media/analysis/twitter/snapshots",
        "timestamp_path": "/data/blockchain-interoperability/blockchain-social-media/analysis/twitter/timestamps",
        "whole_text_path": "/data/blockchain-interoperability/blockchain-social-media/analysis/twitter/whole_text",
        "batch_size": 100000,
        "mainquery": {
            "query": {
                "bool": {
                    "filter": [
                        {
                            "range": {
                                "created_at": {
                                    "gte": "Tue Nov 01 00:00:00 +0000 2022"
                                }
                            }
                        },{
                            "bool": {
                                "must_not": {
                                    "exists": {
                                        "field": "retweeted_status.id"
                                    }
                                },
                                "must": {
                                    "exists": {
                                        "field": "truncated"
                                    }
                                }
                            }
                        }
                    ]
                }
            }
        },
        "fields": [
            "id",
            "created_at",
            "timestamp_ms",
            "user.id",
            "user.description",
            "text",
            "entities.hashtags",
            "truncated",
            "extended_tweet.full_text",
            "extended_tweet.entities.hashtags",
            "quote_count",
            "reply_count",
            "retweet_count",
            "favorite_count",
            "lang",
            "in_reply_to_status_id",
            "n_reply_to_user_id"
        ]
    },
    "tokenize_text": {
        "snapshot_path": "/data/blockchain-interoperability/blockchain-social-media/analysis/twitter/snapshots",
