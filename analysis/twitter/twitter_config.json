{
    "cache_index": {
        "hostname": "http://idea-vm-elasticsearch:9200",
        "index_name": "blockchain-interoperability-attacks",
        "snapshot_path": "/data/blockchain-interoperability/blockchain-social-media/twitter-data/snapshots/snapshots.pkl",
        "timestamp_path": "/data/blockchain-interoperability/blockchain-social-media/twitter-data/snapshots/timestamps.pkl",
        "whole_text_path": "/data/blockchain-interoperability/blockchain-social-media/twitter-data/snapshots/whole_text.pkl",
        "batch_size": 100000,
        "mainquery": {
            "query": {
                "bool": {
                    "filter": [
                        {
                            "range": {
                                "created_at": {
                                    "gte": "Tue Nov 01 00:00:00 +0000 2022"
                                }
                            }
                        },{
                            "bool": {
                                "must_not": {
                                    "exists": {
                                        "field": "retweeted_status.id"
                                    }
                                },
                                "must": {
                                    "exists": {
                                        "field": "truncated"
                                    }
                                }
                            }
                        }
                    ]
                }
            }
        },
        "fields": [
            "id",
            "created_at",
            "timestamp_ms",
            "user.id",
            "user.description",
            "text",
            "entities.hashtags",
            "truncated",
            "extended_tweet.full_text",
            "extended_tweet.entities.hashtags",
            "quote_count",
            "reply_count",
            "retweet_count",
            "favorite_count",
            "lang",
            "in_reply_to_status_id",
            "n_reply_to_user_id"
        ]
    },
    "filter_data": {

    },
    "tokenize_text": {
        "snapshot_path": "/data/blockchain-interoperability/blockchain-social-media/twitter-data/snapshots",
        "token_path": "/data/blockchain-interoperability/blockchain-social-media/twitter-data/tokens",
        "minibatch": 100
    },
    "draw_ngrams_mixed_spam": {
        "n": 3,
        "cloud_max": 100,
        "bar_max": 20,
        "plots_path": "/data/blockchain-interoperability/blockchain-social-media/twitter-data/plots/ngrams",
        "timestamp_path": "/data/blockchain-interoperability/blockchain-social-media/twitter-data/timestamp",
        "token_path": "/data/blockchain-interoperability/blockchain-social-media/twitter-data/tokens",
        "mode": "mixed",
        "separater": " ",
        "spam_idx_path": "/data/blockchain-interoperability/blockchain-social-media/twitter-data/spam_idx.pkl",
        "filter_spam": 0
    },
