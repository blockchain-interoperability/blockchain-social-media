{
    "cache_index": {
        "hostname": "http://idea-vm-elasticsearch:9200",
        "index_name": "blockchain-interoperability-attacks",
        "snapshot_path": "/data/blockchain-interoperability/blockchain-social-media/analysis/twitter/snapshots",
        "batch_size": 100000,
        "mainquery": {
            "query": {
                "bool": {
                    "filter": [
                        {
                            "range": {
                                "created_at": {
                                    "gte": "Tue Nov 01 00:00:00 +0000 2022"
                                }
                            }
                        },{
                            "bool": {
                                "must_not": {
                                    "exists": {
                                        "field": "retweeted_status.id"
                                    }
                                },
                                "must": {
                                    "exists": {
                                        "field": "truncated"
                                    }
                                }
                            }
                        }
                    ]
                }
            }
        },
        "fields": [
            "id",
            "created_at",
            "timestamp_ms",
            "user.id",
            "user.description",
            "text",
            "entities.hashtags",
            "truncated",
            "extended_tweet.full_text",
            "extended_tweet.entities.hashtags",
            "quote_count",
            "reply_count",
            "retweet_count",
            "favorite_count",
            "lang",
            "in_reply_to_status_id",
            "n_reply_to_user_id"
        ]
    },
    "tokenize_text": {
        "snapshot_path": "/data/blockchain-interoperability/blockchain-social-media/analysis/twitter/snapshots",
        "token_path": "/data/blockchain-interoperability/blockchain-social-media/analysis/twitter/tokens",
        "minibatch": 100
    },
    "draw_ngrams_mixed": {
        "n": 3,
        "plots_path": "plots/ngrams",
        "token_path": "/data/blockchain-interoperability/blockchain-social-media/analysis/twitter/tokens",
        "mode": "mixed",
        "separater": " "
    },
    "draw_ngrams_emoji": {
        "n": 3,
        "plots_path": "plots/ngrams",
        "token_path": "/data/blockchain-interoperability/blockchain-social-media/analysis/twitter/tokens",
        "mode": "emoji",
        "separater": ""
    },
    "draw_ngrams_text": {
        "n": 3,
        "plots_path": "plots/ngrams",
        "token_path": "/data/blockchain-interoperability/blockchain-social-media/analysis/twitter/tokens",
        "mode": "text",
        "separater": " "
    },
    "create_sbert_embeddings": {
        "embedder": "all-MiniLM-L6-v2",
        "tokens_path": "/data/blockchain-interoperability/blockchain-social-media/analysis/twitter/tokens/text",
        "bert_embeddings_path": "/data/blockchain-interoperability/blockchain-social-media/analysis/twitter/embeddings"
    },
    "get_umap_embeddings": {
        "n_neighbors": 15,
        "n_components": 5,
        "tokens_path": "/data/blockchain-interoperability/blockchain-social-media/analysis/twitter/tokens/text",
        "bert_embeddings_path": "/data/blockchain-interoperability/blockchain-social-media/analysis/twitter/embeddings/bert.pt",
        "umap_embeddings_path": "/data/blockchain-interoperability/blockchain-social-media/analysis/twitter/embeddings/umap.npy"
    },
    "get_vader_sentiment": {
        "sentiment_type": "vader",
        "snapshot_path": "/data/blockchain-interoperability/blockchain-social-media/analysis/twitter/snapshots",
        "sentiment_path": "/data/blockchain-interoperability/blockchain-social-media/analysis/twitter/sentiment"
    }
    
    
}